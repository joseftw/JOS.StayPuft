{
    "name": "jos-staypuft",
    "description": "A personal blogging theme for Ghost. Beautiful, minimal and responsive.",
    "homepage": "https://github.com/joseftw/JOS.StayPuft",
    "demo": "https://josef.codes",
    "version": "4.1.2",
    "engines": {
        "ghost": ">=3.0.0"
    },
    "license": "MIT",
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "test": "gscan .",
        "preship": "yarn test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
    },
    "author": {
        "name": "Josef Ottosson",
        "email": "no@localhost.local",
        "url": "https://github.com/joseftw"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Personal Blogs"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/joseftw/JOS.StayPuft.git"
    },
    "bugs": "https://github.com/joseftw/JOS.StayPuft/issues",
    "contributors": [
        "https://github.com/joseftw/JOS.StayPuft/graphs/contributors"
    ],
    "devDependencies": {
        "autoprefixer": "6.3.6",
        "beeper": "^1.1.1",
        "cssnano": "3.7.1",
        "gscan": "4.34.0",
        "gulp": "^4.0.0",
        "gulp-filter": "5.1.0",
        "gulp-livereload": "3.8.1",
        "gulp-postcss": "6.1.1",
        "gulp-sourcemaps": "1.6.0",
        "gulp-uglify": "3.0.1",
        "gulp-watch": "4.3.8",
        "gulp-zip": "5.0.2",
        "postcss-color-function": "2.0.1",
        "postcss-custom-properties": "5.0.1",
        "postcss-easy-import": "1.0.1",
        "pump": "3.0.0"
    },
    "config": {
        "card_assets": false,
        "posts_per_page": 5,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "dependencies": {}
}
